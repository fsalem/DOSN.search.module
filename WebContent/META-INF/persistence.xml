<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="DOSN.database.module">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>entities.Friend</class>
		<class>entities.Interest</class>
		<class>entities.User</class>
		<class>entities.UserFriend</class>
		<class>entities.UserFriendPK</class>
		<class>entities.PotentielServer</class>
		<class>entities.UserInterestPrivacyLevel</class>
		<properties>
			<property name="hibernate.archive.autodetection" value="class" />
			<property name="hibernate.hbm2ddl.auto" value="validate" />
		</properties>
	</persistence-unit>
</persistence>
